#!/bin/bash
for subjName in `cat /data/fasttemp/uqtshaw/tomcat/data/subjnames.csv ` ; do
# Script for running ASHS training pipeline
export ASHS_ROOT=/data/lfs2/software/tools/ASHS/ashs-fastashs_beta
mkdir /data/fasttemp/uqtshaw/tomcat/data/derivatives/2_xs_ashs/${subjName}

for x in 01 06 12 ; do
$ASHS_ROOT/bin/ashs_main.sh -a -g -f -I 
done 


$ASHS_ROOT/bin/ashs_train.sh -D /data/home/uqtshaw/testing_folder/long_ashs_with_atlas/manifest.txt -L /data/lfs2/software/ubuntu14/ashs/ashs_atlas_upennpmc_20170810/snap/snaplabels.txt -w /data/home/uqtshaw/testing_folder/long_ashs_with_atlas/testing -C /data/home/uqtshaw/testing_folder/long_ashs_with_atlas/config.txt 
